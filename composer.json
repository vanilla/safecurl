{
    "name": "vanilla/safecurl",
    "description": "A drop-in replacement for 'curl_exec', designed to prevent SSRF attacks.",
    "keywords": [
        "curl",
        "safecurl",
        "safe",
        "ssrf",
        "websec"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Benoist",
            "email": "jeremy.benoist@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Jack W",
            "email": "jack@fin1te.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.5"
    },
    "autoload": {
        "psr-4": {
            "Garden\\SafeCurl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Garden\\SafeCurl\\Tests\\": "tests"
        }
    }
}
